                            DIMENSION (50,20)   :: bhid
   REAL                   , INTENT (OUT ) , &
                            DIMENSION (20,20)   :: bhrd
   TYPE ( all_nml ) , INTENT ( IN )             :: nml
END SUBROUTINE proc_header
